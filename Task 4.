#include <stdio.h>

int main() {
    int n;
    printf("Введіть натуральне число n (1 < n < 150): ");
    scanf("%d", &n);

    // Перевірка введеного числа n на відповідність обмеженням 1 < n < 150
    if (n <= 1 || n >= 150) {
        printf("Неправильне введення. Будь ласка, введіть число у діапазоні 1 < n < 150.\n");
        return 0;
    }

    int count = 0; // Змінна для збереження кількості рівних дільників

    // Шукаємо рівні дільники числа n
    for (int i = 1; i <= n; i++) {
        if (n % i == n / i) {
            count++;
        }
    }

    printf("Кількість рівних дільників числа %d: %d\n", n, count);

    return 0;
}
